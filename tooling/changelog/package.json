    "debug": "^4.3.4",
    "fs-extra": "9.1.0",
    "inquirer": "8.2.4",
    "lodash": "^4.17.21",
    "minimist": "1.2.8",
    "mocha": "7.0.1",
    "semver": "7.5.3",
    "uuid": "8.3.2"
